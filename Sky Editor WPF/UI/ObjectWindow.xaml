<Window x:Class="UI.ObjectWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:res="clr-namespace:SkyEditorWPF.My.Resources"
    Title="{x:Static res:Language.Edit}" Height="490" Width="466">
    <Grid>
        <Menu x:Name="menuMain" IsMainMenu="True" Height="21" Margin="0,0,0,0" VerticalAlignment="Top">
            <MenuItem x:Name="menuFile" Header="{x:Static res:Language.MenuFile}">
                <MenuItem x:Name="menuFileOpen" Header="{x:Static res:Language.MenuFileOpen}" />
                <MenuItem x:Name="menuFileSave" Header="{x:Static res:Language.MenuFileSave}" />
                <MenuItem x:Name="menuFileSaveAs" Header="{x:Static res:Language.MenuFileSaveAs}" />
            </MenuItem>
        </Menu>
        <TabControl x:Name="tcTabs" Margin="0,21,0,0"></TabControl>
    </Grid>
</Window>