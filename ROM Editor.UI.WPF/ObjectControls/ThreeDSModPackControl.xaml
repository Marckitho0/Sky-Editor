<skywpf:ObjectControl x:Class="ObjectControls.ThreeDSModPackControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ROMEditor.UI.WPF"
             xmlns:skywpf="clr-namespace:SkyEditor.UI.WPF;assembly=SkyEditor.UI.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <CheckBox x:Name="chb3DS" Content="Build 3DS" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="chbCIA" Content="Build CIA" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" Visibility="Collapsed"/>
        <CheckBox x:Name="chbHANS" Content="Build HANS" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top"/>

    </Grid>
</skywpf:ObjectControl>
