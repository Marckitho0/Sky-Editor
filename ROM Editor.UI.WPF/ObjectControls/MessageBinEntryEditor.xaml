<skywpf:ObjectControl x:Class="MessageBinEntryEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ROMEditor.UI.WPF"
             xmlns:FileFormats="clr-namespace:ROMEditor.FileFormats.PSMD;assembly=ROMEditor"
             xmlns:skywpf="clr-namespace:SkyEditorWPF.UI;assembly=SkyEditor"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.DataContext>
        <FileFormats:MessageBinStringEntry/>
    </UserControl.DataContext>
    <Grid>
        <TextBox x:Name="txtRaw" TextWrapping="Wrap" Text="{Binding Entry}" AcceptsReturn="True"/>
    </Grid>
</skywpf:ObjectControl>
