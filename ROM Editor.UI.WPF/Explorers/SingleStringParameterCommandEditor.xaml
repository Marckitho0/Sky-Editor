<skyWPF:DataBoundObjectControl x:Class="Explorers.SingleStringParameterCommandEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ROMEditor.UI.WPF"
             xmlns:skyWPF="clr-namespace:SkyEditor.UI.WPF;assembly=SkyEditor.UI.WPF"
             xmlns:commands="clr-namespace:ROMEditor.FileFormats.Explorers.Script.Commands;assembly=ROMEditor"
             mc:Ignorable="d" 
             TargetType="{x:Type commands:ISingleStringParamCommand}"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <skyWPF:ObjectControlPlaceholder Grid.Row="3" CurrentPluginManager="{Binding Path=CurrentPluginManager,RelativeSource={RelativeSource AncestorType=skyWPF:DataBoundObjectControl}}" ObjectToEdit="{Binding Path=(commands:ISingleStringParamCommand.Line)}" EnableTabs="False"/>
    </Grid>
</skyWPF:DataBoundObjectControl>
