<Window x:Class="SkyEditorWindows.NewNameWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="New File" Height="143" Width="345">
    <Grid>
        <Label x:Name="lbGame" Content="Name" HorizontalAlignment="Left" Margin="10,0,0,47" VerticalAlignment="Bottom" />
        <TextBox x:Name="txtName" Margin="56,0,10,47" Height="22" VerticalAlignment="Bottom" />
        <Label x:Name="lbGameSelectorQuestion" Content="What should the new file be called?" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" />
        <Button x:Name="btnOK" Content="OK" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="Button_Click" />
    </Grid>
</Window>
