<skywpf:ObjectControl x:Class="Tabs.EpisodeActivePokemonTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:skywpf="clr-namespace:SkyEditorWPF.UI;assembly=SkyEditor"
             xmlns:res="clr-namespace:SaveEditor.UI.WPF.My.Resources"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ListBox x:Name="lbActivePokemon" Margin="0,0,0.4,30" SelectionMode="Extended" />
        <Button x:Name="btnEditActivePokemon" Content="{x:Static res:Language.Edit}" Margin="10,0,0,5" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="45" />
    </Grid>
</skywpf:ObjectControl>